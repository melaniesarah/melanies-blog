<h1>New Article</h1>
<img src="http://cliparts.co/cliparts/pT5/rB9/pT5rB9Kgc.png" alt="Drama masks"  height="100" width="100"/>
<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit 'Save Article', id: "save-button" %>
  </p> 
<% end %>

<%= link_to 'Back', articles_path, class: "big button"%>